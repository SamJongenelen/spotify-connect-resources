<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes ">
<title>%_STATION_PAGE_TITLE_%</title>
<link rel="stylesheet" href="websetup_cmn.css" type="text/css">
<link rel="stylesheet" href="websetup_cmn_smt.css" type="text/css">
<link rel="stylesheet" href="websetup_station.css" type="text/css">
<script language="JavaScript" type="text/JavaScript" src="websetup_cmn.js"></script>
<script language="JavaScript" type="text/JavaScript" src="websetup_station.js"></script>
<style type="text/css">
<!--
td.info_no,td.info_name,td.info_url{ font-size: small; }
-->
</style>
</head>
<body>
<table class="top">
  <tr class="header">
    <td class="header"> 
      <table class="header">
        <tr class="tab">
          <td class="header_tab"><a href="%_COMMON_PATH_UINFO_%" class="header_tab"><img src="images/ic_device_info_off.png" width="58" height="52" alt="%_TAB_UINFO_%" class="tab"></a></td>
          <td class="header_tab_crnt"><img src="images/ic_my_favorites_on.png" width="58" height="52" alt="%_TAB_STATION_%" class="tab"></td>
          <td class="header_tab"><a href="%_COMMON_PATH_NETWORK_%" class="header_tab"><img src="images/ic_network_setting_off.png" width="58" height="52" alt="%_TAB_NETWORK_%" class="tab"></a></td>
          <td class="header_tab"><a href="%_COMMON_PATH_ACCOUNT_%" class="header_tab"><img src="images/ic_account_setting_off.png" width="58" height="52" alt="%_TAB_ACCOUNT_%" class="tab"></a></td>
        </tr>
      </table>
    </td>
  </tr>
  <tr> 
    <td class="body_top">
      <form action="./%_COMMON_PATH_STATION_%" method="POST" name="station_list">
        <table class="body">
          <tr> 
            <td colspan="4" class="middle_title">
              <table class="subtitle">
                <tr>
                  <td class="refresh">
                    <a href="./%_COMMON_PATH_STATION_%" onTouchEnd="MM_swapImgRestore()" onTouchStart="MM_swapImage('Image5','','images/btn_refresh_pressed.png',1)"><img src="images/btn_refresh_normal.png" name="Image5" width="32" height="32" alt="%_BUTTON_REFRESH_%" class="refresh"></a>
                  </td>
                  <td class="subtitle_left">
                    <strong class="middle_title">%_STATION_MIDDLE_TITLE_%</strong>
                  </td>
                  <td class="subtitle_right">
                    <button type="submit" name="save" value="SAVE" class="save">%_BUTTON_SAVE_%</button>
                  </td>
                </tr>
              </table>
            </td>
          </tr>
          <tr><td colspan="4"><hr class="caption" noshade></td></tr>
          <tr class="backline">
            <td class="info_no">%_STATION_LIST_NO_TITLE_%</td>
            <td class="info_name">%_STATION_LIST_NAME_TITLE_%</td>
            <td class="info_url">%_STATION_LIST_URL_TITLE_%</td>
            <td class="info_del"></td>
          </tr>
%_STATION_LIST_STATIONS_INFO_%        </table>
      </form>
    </td>
  </table>
<div class="foot"> </div>
</body>
</html>
